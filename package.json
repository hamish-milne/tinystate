{
  "name": "@hmilne/tinystate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "build-lib": "esbuild ./lib/*.ts --outdir=./dist --drop-labels=TEST --legal-comments=none",
    "build-lib-min": "esbuild --bundle --minify ./lib/index.ts --external:preact --outfile=./dist/min.js --mangle-props=^_ --drop-labels=TEST --format=esm --legal-comments=none --sourcemap",
    "gzip-stats": "node -e \"console.log('Compressed size: '+require('zlib').gzipSync(require('fs').readFileSync('./dist/min.js')).length)\""
  },
  "dependencies": {
    "preact": "^10.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@biomejs/biome": "^2.2.0",
    "@preact/preset-vite": "^2.10.2",
    "@types/babel__core": "^7.20.5",
    "@types/node": "^20.19.10",
    "@types/react": "^19.1.10",
    "@vitest/coverage-v8": "^3.2.4",
    "@yarnpkg/sdks": "^3.2.2",
    "esbuild": "0.25.8",
    "happy-dom": "^18.0.1",
    "typescript": "~5.9.2",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  },
  "packageManager": "yarn@4.9.2"
}
