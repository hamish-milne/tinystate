{
  "name": "@hmilne/tinystate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "build-lib": "esbuild ./lib/*.ts --outdir=./dist --drop-labels=TEST --legal-comments=none",
    "build-lib-min": "esbuild --bundle --minify ./lib/index.ts --external:preact --outfile=./dist/min.js --mangle-props=^_ --drop-labels=TEST --format=esm --legal-comments=none --sourcemap"
  },
  "dependencies": {
    "preact": "^10.26.9"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@biomejs/biome": "^2.1.4",
    "@preact/preset-vite": "^2.10.2",
    "@types/babel__core": "^7",
    "@vitest/coverage-v8": "^3.2.4",
    "@yarnpkg/sdks": "^3.2.2",
    "esbuild": "0.25.8",
    "typescript": "~5.9.2",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  },
  "packageManager": "yarn@4.9.2"
}
